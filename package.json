{
  "name": "react-native-cpp-turbo",
  "version": "1.0.5",
  "description": "React Native Turbo Module with C++ shared library",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-cpp-turbo.podspec"
  ],
  "scripts": {
    "prepare": "bob build",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\""
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "cpp",
    "turbo-module"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jahskee/react-native-cpp-turbo.git"
  },
  "author": "Jaison Francisco <jahskee@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jahskee/react-native-cpp-turbo/issues"
  },
  "homepage": "https://github.com/jahskee/react-native-cpp-turbo#readme",
  "peerDependencies": {
    "react": "*",
    "react-native": "^0.79.0"
  },
  "devDependencies": {
    "@react-native/eslint-config": "^0.73.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "eslint": "^8.0.0",
    "react": "18.2.0",
    "react-native": "0.79.0",
    "react-native-builder-bob": "^0.23.0",
    "typescript": "^5.0.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "codegenConfig": {
    "name": "RNCppTurboSpec",
    "type": "modules",
    "jsSrcsDir": "src"
  }
}
